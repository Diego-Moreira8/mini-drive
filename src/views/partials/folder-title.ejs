    <%= locals.folder.rootOfUserId ? "Meus Arquivos"  : locals.folder.name %>
  </h1>

  <% if (!locals.folder.rootOfUserId) { %>
    <div class="options">
      <button type="button">Renomear</button>
      <button type="button">Apagar</button>
    </div>
  <% } %>
</div>